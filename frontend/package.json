{
  "name": "mastopod",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "format": "prettier --write ./src",
    "link-semapps-packages": "yalc link @semapps/activitypub-components @semapps/auth-provider @semapps/date-components @semapps/field-components @semapps/geo-components @semapps/input-components @semapps/list-components @semapps/markdown-components @semapps/semantic-data-provider",
    "unlink-semapps-packages": "yalc remove --all && rm -rf node_modules/@semapps && yarn install --force",
    "link-activitypods-packages": "yalc link @activitypods/react",
    "unlink-activitypods-packages": "yalc remove --all && rm -rf node_modules/@activitypods && yarn install --force",
    "link-packages": "yarn run link-semapps-packages && yarn run link-activitypods-packages",
    "unlink-packages": "yalc remove --all && rm -rf node_modules/@semapps && rm -rf node_modules/@activitypods && yarn install --force"
  },
  "dependencies": {
    "@activitypods/ldo-shapes": "^0.2.0",
    "@activitypods/react": "2.1.0",
    "@activitypods/shape-definitions": "^0.2.1",
    "@ldo/connected": "^1.0.0-alpha.32",
    "@ldo/connected-solid": "^1.0.0-alpha.32",
    "@mui/styles": "^6.4.8",
    "@mui/system": "^7.3.2",
    "@rdfjs/parser-n3": "^2.1.0",
    "@semapps/activitypub-components": "1.1.0",
    "@semapps/auth-provider": "1.1.0",
    "@semapps/semantic-data-provider": "1.1.0",
    "@tanstack/react-query": "^5.85.9",
    "@tiptap/extension-hard-break": "^2.10.2",
    "@tiptap/extension-mention": "^2.10.2",
    "@tiptap/extension-placeholder": "^2.10.2",
    "@tiptap/suggestion": "^2.10.2",
    "dayjs": "^1.11.10",
    "isobject": "^4.0.0",
    "ra-core": "^5.11.0",
    "ra-input-rich-text": "^5.8.3",
    "ra-language-french": "^5.8.3",
    "ra-ui-materialui": "^5.11.0",
    "rdf-ext": "^2.6.0",
    "react": "^18.2.0",
    "react-admin": "^5.8.3",
    "react-dom": "^18.2.0",
    "react-sticky-box": "^2.0.5",
    "readable-stream": "^4.7.0",
    "readable-web-to-node-stream": "^5.0.0",
    "shacl-engine": "^1.0.2",
    "tss-react": "^4.9.19",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.27",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6",
    "vite": "^5.1.5",
    "yalc": "1.0.0-pre.53"
  }
}
